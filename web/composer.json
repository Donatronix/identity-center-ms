{
    "name": "gic/sumra-id-api",
    "description": "Global Identity Centre. Sumra ID API",
    "type": "project",
    "private": true,
    "require": {
        "php": "^7.4|^8.0",
        "darkaonline/l5-swagger": "~8",
        "fruitcake/laravel-cors": "~2",
        "guzzlehttp/guzzle": "~7",
        "laravel/framework": "~8",
        "laravel/passport": "~10",
        "laravel/tinker": "~2",
        "sumra/sdk": "*"
    },
    "require-dev": {
        "facade/ignition": "~2",
        "fakerphp/faker": "~1",
        "laravel/sail": "~1",
        "mockery/mockery": "~1",
        "nunomaduro/collision": "~5",
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type":"path",
            "url": "../sumra-sdk",
            "options": {
                "symlink": false
            }
        }
    ]
}
