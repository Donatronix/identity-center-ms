{
    "name": "gic/sumra-id-api",
    "description": "Global Identity Centre. Sumra ID API",
    "type": "project",
    "private": true,
    "require": {
        "php": "^8.0",
        "darkaonline/l5-swagger": "~8",
        "fruitcake/laravel-cors": "~2",
        "laravel/framework": "~8",
        "laravel/passport": "~10",
        "laravel/tinker": "~2",
        "sumra/sdk": "*"
        "php": "^8.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "doctrine/dbal": "~3",
        "dusterio/lumen-passport": "^0.3.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/redis": "*",
        "laravel/lumen-framework": "~8",
        "predis/predis": "^1.1",
        "sumra/sdk": "*"
    },
    "require-dev": {
        "fakerphp/faker": "~1",
        "mockery/mockery": "~1"

    },
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../sumra-sdk",
            "options": {
                "symlink": false
            }
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
